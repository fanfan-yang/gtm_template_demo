// Require the necessary APIs
const logToConsole = require('logToConsole');
const injectScript = require('injectScript');

const copyFromWindow = require('copyFromWindow');
const setInWindow = require('setInWindow');
const callInWindow = require('callInWindow');
const aliasInWindow = require('aliasInWindow');
const createQueue = require('createQueue');

const getCookieValues = require('getCookieValues');




const ttq = copyFromWindow("ttq");

const eventsWithContent = ['ViewContent','AddToCart','CompletePayment'];


function trigger_event(){
  
  const am_data={};
  
  callInWindow("ttq.page");
  
  if( typeof data.advanced_matching !== 'undefined' ){
    
    if( data.advanced_matching == 'am_text' ){
        if( typeof data.email !== 'undefined' ) {am_data.email = data.email;}
        if( typeof data.mobile !== 'undefined' ) {am_data.phone_number = data.mobile;}
    }
    
    if( data.advanced_matching == 'am_sha' ){
        if( typeof data.email !== 'undefined' ) {am_data.sha256_email = data.email;}
        if( typeof data.mobile !== 'undefined' ) {am_data.sha256_phone_number = data.mobile;}
    }
    
    if( data.enable_cookies == 'no' || data.enable_cookie_value == 0){
      callInWindow("ttq.disableCookie");
    }else{
      callInWindow("ttq.enableCookie");
    }

    //logToConsole( 'the value of : ' + am_data.sha256_email );
    callInWindow("ttq.identify", am_data );

  }
  
  

  if(data.event != "All Pages" ){
    
    if( typeof data.content !== 'undefined' ){
      
      if( data.event == 'CompletePayment'){
        
        const products_data = { contents:data.content, value:data.order_value, currency:data.currency };
        callInWindow("ttq.track", data.event , products_data );
      }else if( data.content == 'pass-specific-data' ){
        
        const product_data = { };
        
        if( typeof data.content_id !== 'undefined' ){ product_data.content_id = data.content_id;}
        
        if( typeof data.content_name !== 'undefined' ){ product_data.content_name = data.content_name;}

        if( typeof data.content_category !== 'undefined' ){ product_data.content_category = data.content_category;}
        
        if( typeof data.content_type !== 'undefined' ){ product_data.content_type = data.content_type;}
        
        if( typeof data.price !== 'undefined' ){ product_data.price = data.price;}
        
        if( typeof data.quantity !== 'undefined' ){ product_data.quantity = data.quantity;}
        
        if( typeof data.value !== 'undefined' ){ product_data.value = data.value;}
        
        if( typeof data.content_currency !== 'undefined' ){ product_data.currency = data.content_currency;}

        callInWindow("ttq.track", data.event , product_data );
        /*
        const x = callInWindow("ttq.instance", data.pixel_id);
        logToConsole( x );
        x.track("ViewContent");
          x.callInWindow("track", data.event , data.content );
        */
      }else{
        callInWindow("ttq.track", data.event , data.content );
      }

      
      //logToConsole( ' passing data ');
    }else{
      callInWindow("ttq.track", data.event );
      //logToConsole( ' no data to pass ');
    }
    
  }
  
}


function OnSuccess(){
  
  const ttqLoad = copyFromWindow("ttq.load");
  const ttqTrack = copyFromWindow("ttq.track");
  const ttqIdentify = copyFromWindow("ttq.identify");
  const ttqInstance = copyFromWindow("ttq.instance");
  
  callInWindow("ttq.load", data.pixel_id);
  //callInWindow("ttq.page");
  
  trigger_event();
  
  /*
  if(data.event != "All Pages" && eventsWithContent.indexOf(data.event) > -1 ){
    callInWindow("ttq.track", data.event , data.content );
  }else if(data.event != 'All Pages'){
     callInWindow("ttq.track", data.event );
  }
  */
  
}


function OnFailure(){
  logToConsole('failed to load');
}

if( !ttq ){
  injectScript('https://www.dinteraction.com/gtm_test/tiktok.js?id123', OnSuccess , OnFailure );
}else{
  
  trigger_event();
  
